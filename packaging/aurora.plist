<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.aurora.auroraos</string>
  <key>Program</key>
  <string>/usr/bin/python3</string>
  <key>ProgramArguments</key>
  <array>
    <string>/usr/bin/python3</string>
    <string>/Users/YOURUSERNAME/Aurora-x/aurora_os.py</string>
    <string>start</string>
  </array>
  <key>WorkingDirectory</key>
  <string>/Users/YOURUSERNAME/Aurora-x</string>
  <key>EnvironmentVariables</key>
  <dict>
    <key>AURORA_API_TOKEN</key>
    <string>aurora-dev-token</string>
  </dict>
  <key>RunAtLoad</key>
  <true/>
  <key>KeepAlive</key>
  <true/>
  <key>StandardOutPath</key>
  <string>/Users/YOURUSERNAME/Aurora-x/aurora_logs/launchd.log</string>
  <key>StandardErrorPath</key>
  <string>/Users/YOURUSERNAME/Aurora-x/aurora_logs/launchd.err</string>
</dict>
</plist>
